@model IEnumerable<Thing>
@{
    ViewData["Title"] = "Home";
    @*<link href='https://fonts.googleapis.com/css?family=Josefin+Sans' rel='stylesheet' type='text/css'>*@
}
<div class="fixed-bottom" style="display: inline-block; padding-bottom: 60px; padding-left: 10px; z-index: -12; right: auto; width: 105">
    <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner border-standard-free">
            <div class="carousel-item active">
                <img class="square" src="/images/PortalBenjaminDark.png" alt="Portal">
            </div>
            @foreach(var thing in Model)
            {
            <div class="carousel-item">
                <img class=" square" src="@thing.ImagePath" alt="@thing.Name">
            </div>
            }
        </div>
    </div>
</div>

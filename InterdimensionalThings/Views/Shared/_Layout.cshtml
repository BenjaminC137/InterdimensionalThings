<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"] - InterdimensionalThings</title>

    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css"/>
</head>

<body>
    <header>
        <p>&copy; 2018 - InterdimensionalThings</p>
    </header>
    <hr />

    <nav class="navbar fixed-bottom navbar-dark bg-dark navbar-expand-lg">
        <a class="navbar-custom" asp-controller="Home" asp-action="Index">
            <img src="/images/NeuronLightBlue.png" width="auto" height="30" class="d-inline-block align-top" alt="Neuron">
            Interdimensional Things
        </a>
        <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
                
                    <a class="btn" data-toggle="collapse" href="#collapseLanguage" role="button" aria-expanded="false" aria-controls="collapseExample">
                        Language
                    </a>
                
                <div class="collapse" id="collapseLanguage">
                    <div class="card card-body">
                        English<br />
                        Pig-Lattin<br />
                        Bird-Person<br />
                    </div>
                </div>
            </li>

            <li class="nav-item active">
                
                    <a class="btn" data-toggle="collapse" href="#collapseCurrency" role="button" aria-expanded="false" aria-controls="collapseExample">
                        Currency
                    </a>
                
                <div class="collapse" id="collapseCurrency">
                    <div class="card card-body">
                        USD<br />
                        Blemflarck<br />
                        Brapple<br />
                        Flurbo<br />
                        Glem<br />
                        Schmeckle<br />
                        Smidgen<br />
                        Dollar
                    </div>
                </div>
            </li>
        </ul>








        <button class="navbar-toggler custom-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent1" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            Open<span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent1">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" asp-controller="Home" asp-action="Index">Home <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-controller="Home" asp-action="About">About</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-controller="Home" asp-action="Contact">Contact</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-controller="Things" asp-action="index">Things</a>
                </li>
                @*
                <li class="nav-item">
                    <a class="nav-link" asp-controller="Things" asp-action="prototypes" asp-route-category="prototypes">no</a>
                </li>*@
            </ul>




            <div class="nav-item active">
                
                    <a class="btn" data-toggle="collapse" href="#collapseSearch" role="button" aria-expanded="false" aria-controls="collapseExample">
                        Search
                    </a>
                
                <div class="collapse" id="collapseSearch">
                    <div class="card card-body">
                        Anim nesciunt sapiente ea proident.
                    </div>
                    <form class="form-inline my-2 my-lg-0">
                        <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
                    </form>
                </div>
            </div>

        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()
        <footer></footer>
    </div>

    @RenderSection("Scripts", required: false)

    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <script src="~/js/site.js"></script>

</body>
</html>
